{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "header",
                    "key": "x-forwarded-proto",
                    "value": "http"
                }
            ],
            "destination": "https://tomweb.dev/:path*",
            "permanent": true
        }
    ]
}